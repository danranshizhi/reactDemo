{
  "presets": [
    [
      "babel-preset-env",
      {
        "targets": {
          "browser": [
            "> 1%",
            "last 2 versions"
          ]
        }
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib", // default: lib
        "style": true,
      }
    ]
  ],
}